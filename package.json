{
  "name": "@skitscript/parser-nodejs",
  "description": "A Skitscript document parser targeting NodeJS.",
  "version": "0.0.0",
  "engines": {
    "node": ">=16.11.1"
  },
  "engineStrict": true,
  "repository": {
    "type": "git",
    "url": "https://github.com/skitscript/parser-nodejs"
  },
  "publishConfig": {
    "access": "public"
  },
  "private": false,
  "license": "MIT",
  "devDependencies": {
    "@skitscript/types-nodejs": "0.0.29",
    "@types/jasmine": "3.10.3",
    "@types/node": "16.11.19",
    "@typescript-eslint/eslint-plugin": "5.9.1",
    "@typescript-eslint/parser": "5.9.1",
    "del-cli": "4.0.1",
    "eslint": "8.6.0",
    "eslint-config-prettier": "8.3.0",
    "eslint-plugin-prettier": "4.0.0",
    "jasmine": "4.0.1",
    "npm-run-all": "4.1.5",
    "nyc": "15.1.0",
    "prettier": "2.5.1",
    "typescript": "4.5.4"
  },
  "scripts": {
    "buildClean": "del-cli \"**/*.js\" \"**/*.d.ts\" \"!**/node_modules/**/*\" \"!**/submodules/**/*\"",
    "buildLint": "eslint \"**/*.ts\"",
    "buildLintFix": "eslint --fix \"**/*.ts\"",
    "buildTypeScript": "tsc",
    "buildJasmine": "jasmine --config=jasmine.json",
    "buildNyc": "nyc --check-coverage --all --exclude \"submodules\" --exclude \"**/*.ts\" --exclude \"**/*.d.ts\" --exclude \"**/unit.*\" --lines 100 --functions 100 --branches 100 --statements 100 npm run-script buildJasmine",
    "test": "npm-run-all buildClean buildLintFix buildTypeScript buildNyc"
  },
  "files": [
    "**/index.js",
    "**/index.d.ts"
  ],
  "types": "index.d.ts",
  "sideEffects": false,
  "peerDependencies": {
    "@skitscript/types-nodejs": "0.0.29"
  }
}
